{
   "id": "chronos",
   "instances": 1,
   "cpus": 1,
   "mem": 2048,
   "uris": [ ],
   "args": [
      "/usr/bin/chronos", "run_jar", 
      "--master", "zk://10.105.0.1:2181,10.105.0.3:2181,10.105.0.5:2181,10.105.0.7:2181,10.105.0.9:2181/mesos",
      "--zk_hosts", "10.105.0.1:2181,10.105.0.3:2181,10.105.0.5:2181,10.105.0.7:2181,10.105.0.9:2181",
      "--http_port", "8080"
   ],
   "container": {
      "type":"DOCKER",
      "volumes": [],
      "docker":{
         "image": "mesosphere/chronos:chronos-2.4.0-0.1.20151007110204.ubuntu1404-mesos-0.24.1-0.2.35.ubuntu1404",
         "network": "BRIDGE",
         "portMappings" : [
            {
               "containerPort": 8080, 
               "hostPort": 0,        
               "protocol": "tcp"
            }
         ],
         "parameters": [
            {"key": "name", "value": "chronos"}
         ]
      }
   },
   "shell": "false",
   "maxLauchDelaySeconds": 15400
}
