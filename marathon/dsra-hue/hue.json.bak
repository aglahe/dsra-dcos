{
  "id": "hue",
  "cpus": 1.0,
  "mem": 2048.0,
  "cmd": "build/env/bin/hue runserver 0.0.0.0:8000",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "hub.dsra.local:5000/dsra/hue:3.7.0.1",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8000, "hostPort": 8000 },
        { "containerPort": 9999, "hostPort": 9999 },
        { "containerPort": 8020, "hostPort": 8020 },
        { "containerPort": 50070, "hostPort": 50070 }
      ]
    }
  },
  "uris": [
     "file:///etc/docker.tar.gz"
  ]
}

