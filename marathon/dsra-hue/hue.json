{
  "id": "hue",
  "cpus": 1.0,
  "mem": 2048.0,
  "cmd": "build/env/bin/hue runserver 0.0.0.0:8000",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "hub.dsra.local:5000/dsra/hue:3.9.0",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8000, "hostPort": 0, "protocol": "tcp" },
        { "containerPort": 50070, "hostPort": 50070 }
      ]
    }
  },
  "uris": [
     "file:///etc/docker.tar.gz"
  ],
  "maxLaunchDelaySeconds": 7200
}

