{
  "id": "hue",
  "cpus": 1.0,
  "mem": 2048.0,
  "instances": 1,
  "args": ["hue"],
  "env": {
     "CLUSTER_NAME": "dsra",
     "HTTPFS_SERVER": "httpfs.dsra.local",
     "HTTPFS_PORT": "32000"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "hub.dsra.local:5000/dsra/hue:3.9.0",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8000, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "uris": [
     "file:///etc/docker.tar.gz"
  ],
  "maxLaunchDelaySeconds": 7200
}
