{
  "id": "/dsra/zk-web",
  "cpus": 1.0,
  "mem": 2048.0,
  "instances": 1,
  "args": ["lein run"],
  "env": {
     "CLUSTER_NAME": "dsra",
     "HTTPFS_SERVER": "httpfs-dsra.marathon.slave.mesos",
     "HTTPFS_PORT": "31400"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "goodguide/zk-web",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "protocol": "tcp" }
      ]
    }
  },
  "maxLaunchDelaySeconds": 7200
}
